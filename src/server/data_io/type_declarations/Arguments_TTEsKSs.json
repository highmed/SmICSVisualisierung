{
  "$id": "args/Arguments_TTEsKSs",
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "starttime": {
      "type": "string",
      "moment": {
        "validate": [
          {
            "test": "isSameOrAfter",
            "value": "1980-01-01T00:00:00Z"
          }
        ]
      }
    },
    "endtime": {
      "type": "string",
      "moment": {
        "validate": [
          {
            "test": "isSameOrAfter",
            "value": {
              "$data": "1/starttime"
            }
          },
          {
            "test": "isSameOrBefore",
            "value": "2100-01-01T00:00:00Z"
          }
        ]
      }
    },
    "pathogen": {
      "type": "string"
    },
    "hospital": {
      "type": "string",
      "minimum": 0
    },
    "stationList": {
      "type": "array",
      "description": "station IDs",
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["starttime", "endtime", "pathogen"]
}
